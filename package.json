{
  "name": "@omichalo/sqyping-mui-theme",
  "version": "0.1.8",
  "description": "Production-ready MUI theme for SQY PING React/Next.js applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ssr": {
      "import": "./dist/ssr-exports.esm.js",
      "require": "./dist/ssr-exports.js",
      "types": "./dist/ssr-exports.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "demo:dev": "cd demo && npm run dev",
    "demo:build": "cd demo && npm run build",
    "demo:deploy": "cd demo && npm run deploy"
  },
  "keywords": [
    "mui",
    "material-ui",
    "theme",
    "react",
    "nextjs",
    "typescript",
    "sqyping",
    "design-system"
  ],
  "author": "SQY PING",
  "license": "MIT",
  "dependencies": {
    "@fontsource-variable/figtree": "^5.0.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omichalo/sqyping-mui-theme.git"
  },
  "homepage": "https://github.com/omichalo/sqyping-mui-theme#readme",
  "bugs": {
    "url": "https://github.com/omichalo/sqyping-mui-theme/issues"
  }
}
